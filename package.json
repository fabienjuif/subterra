{
  "name": "subterra",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@myrtille/mutate": "^0.8.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "classnames": "^2.2.6",
    "immer": "^6.0.2",
    "node-sass": "^4.13.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "use-immer": "^0.3.5",
    "uuid": "^7.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,md,json}\" --write",
    "format:check": "prettier \"**/*.{ts,tsx,js,jsx,md,json}\" --check",
    "ci": "CI=1 run-p format:check test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "husky": ">=4",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,md,json}": "prettier --write"
  },
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "singleQuote": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  }
}
